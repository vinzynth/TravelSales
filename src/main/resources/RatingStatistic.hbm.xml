<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="at.tug.oad.travelsales.model.impl">
	<class name="RatingStatistic" table="RATING_STATISTIC">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="userId" column="user_id" type="int" />
		<property name="baseGameId" column="base_game_id" type="int" />
		<property name="rating" column="rating" type="double" />
	</class>
</hibernate-mapping>