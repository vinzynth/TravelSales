<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="at.tug.oad.travelsales.model.impl">
	<class name="Point" table="POINT">
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="name" column="name" type="string" />
		<property name="iconPath" column="icon_path" type="string" />
		<property name="x" column="x" type="double" />
		<property name="y" column="y" type="double" />
	</class>
</hibernate-mapping>